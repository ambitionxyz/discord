  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.1",
    "next-auth": "^4.24.4",
    "postcss": "^8.4.31",
    "react-hook-form": "^7.48.2",
    "tailwind-merge": "^2.0.0",
    "uploadthing": "^5.7.4",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.6"
  }
